{
	"manifest_version": 2,
	"name": "BlockView",
	"version": "0.1",
	"icons" : { "128":"icon128.png" },
	"description": "Allows you to visit youtube and reddit by URL",
	"background":{
		"scripts": ["background.js"]
	},
	"browser_action":{
		"default_title": "BlockView",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions":[
		 "tabs",
		 "storage"
	]
}